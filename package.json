{
  "name": "@spiriit/vite-plugin-svg-spritemap",
  "type": "module",
  "version": "2.3.0",
  "packageManager": "pnpm@9.5.0",
  "description": "Generates symbol-based SVG spritemap from all .svg files in a directory",
  "author": "Spiriit",
  "license": "MIT",
  "homepage": "https://github.com/SpiriitLabs/vite-plugin-svg-spritemap",
  "repository": {
    "type": "git",
    "url": "https://github.com/SpiriitLabs/vite-plugin-svg-spritemap"
  },
  "bugs": "https://github.com/SpiriitLabs/vite-plugin-svg-spritemap/issues",
  "keywords": [
    "svg",
    "sprites",
    "spritemap",
    "symbols",
    "icons",
    "sass",
    "scss",
    "css",
    "less",
    "stylus",
    "variables",
    "vite",
    "vite-plugin"
  ],
  "exports": {
    ".": {
      "import": {
        "types": "./dist/index.d.ts",
        "default": "./dist/index.js"
      },
      "require": {
        "types": "./dist/index.d.cts",
        "default": "./dist/index.cjs"
      }
    },
    "./client": {
      "types": "./dist/client.d.ts"
    }
  },
  "main": "./dist/index.js",
  "module": "./dist/index.js",
  "types": "./dist/index.d.ts",
  "files": [
    "dist"
  ],
  "engines": {
    "node": "^14.18.0 || >=16.0.0"
  },
  "scripts": {
    "build": "tsup",
    "build:watch": "tsup --watch",
    "test": "vitest",
    "coverage": "vitest run --coverage",
    "lint": "eslint .",
    "lint:fix": "eslint . --fix",
    "typecheck": "tsc --noEmit --skipLibCheck",
    "prepublishOnly": "pnpm build"
  },
  "peerDependencies": {
    "vite": "^4.0.0 || ^5.0.0",
    "vue": "^3.0.0"
  },
  "peerDependenciesMeta": {
    "vue": {
      "optional": true
    }
  },
  "dependencies": {
    "@xmldom/xmldom": "^0.8.10",
    "fast-glob": "^3.3.2",
    "hash-sum": "^2.0.0",
    "mini-svg-data-uri": "^1.4.4",
    "svgo": "^3.3.2"
  },
  "devDependencies": {
    "@antfu/eslint-config": "2.24.1",
    "@types/hash-sum": "^1.0.2",
    "@types/node": "^22.0.2",
    "@types/xmldom": "^0.1.34",
    "@vitejs/plugin-vue": "^5.1.2",
    "@vitest/coverage-v8": "^2.0.5",
    "c8": "^10.1.2",
    "eslint": "^9.8.0",
    "playwright": "^1.45.3",
    "rollup": "^4.19.2",
    "svg-element-attributes": "^2.1.0",
    "tsup": "^8.2.3",
    "typescript": "^5.5.4",
    "vite": "^5.3.5",
    "vitest": "^2.0.5",
    "vue": "^3.4.35"
  }
}
